
<% if friend.user == current_user %>
<tr>
 <td>
 <%= link_to friend.first_name+' '+friend.last_name, friend %>
</td>
 <td><%= friend.email %></td>
 <td><%= friend.phone %></td>
 <td><%= friend.twitter %></td>
 <td><%= link_to "Edit", edit_friend_path(friend) %></td>
 <td><%= button_to "delete", friend, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-outline-danger btn-sm" %></td>
<tr>
<% end %>